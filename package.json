{
  "name": "polymer-setup",
  "version": "0.1.0",
  "main": "src/index.js",
  "license": "MIT",
  "author": "laura.merlo@frontmen.nl",
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --env.NODE_ENV=production --optimize-minimize",
    "dev": "webpack-dev-server --hot --inline",
    "pretest": "yarn build",
    "test": "yarn lint && wct --npm && yarn test:lighthouse",
    "test:lighthouse": "concurrently --kill-others \"http-server dist\" \"lighthouse --output html --output-path ./report/report.html --view http://localhost:8080/\" ",
    "test:travis": "yarn pretest && yarn lint && wct --npm",
    "clean:lighthouse": "rm -rf ./report/*",
    "posttest": "yarn rmbower",
    "lint": "stylelint src/components/**/*.pcss",
    "start": "yarn build && http-server dist"
  },
  "engines": {
    "node": ">= 8.0 < 10.15"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "autoprefixer": "9.1.4",
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "8.0.3",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "4.0.1",
    "copy-webpack-plugin": "4.5.2",
    "cssnano": "4.1.4",
    "ejs-loader": "0.3.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "lighthouse": "3.2.1",
    "postcss": "7.0.5",
    "postcss-cssnext": "3.1.0",
    "postcss-custom-media": "6.0.0",
    "postcss-discard-comments": "4.0.1",
    "postcss-easy-import": "3.0.0",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.0",
    "postcss-nesting": "7.0.0",
    "postcss-reporter": "6.0.0",
    "postcss-selector-not": "4.0.0",
    "script-ext-html-webpack-plugin": "2.0.1",
    "standard-version": "4.4.0",
